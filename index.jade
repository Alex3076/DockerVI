        <!-- page specific plugin scripts -->
        script(type="text/javascript" src="./assets/js/jquery-ui-1.10.3.full.min.js")
        script(type="text/javascript" src="./assets/js/jquery.ui.touch-punch.min.js")
        script(type="text/javascript" src="./assets/js/date-time/bootstrap-datepicker.min.js")
